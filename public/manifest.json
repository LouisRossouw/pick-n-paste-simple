{
  "manifest_version": 3,
  "name": "Pick n Paste",
  "description": "Pick emojis & Tailwind colors!",
  "version": "1.2.0",
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "side_panel": {
    "default_path": "src/side-panel/index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "128": "pick_n_paste_logo_128.png"
  },
  "permissions": [
    "storage",
    "sidePanel"
  ]
}