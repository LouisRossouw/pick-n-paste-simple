{
"manifest_version": 3,
"name": "Pick n Paste",
"description": "Pick emojis & Tailwind colors!",
"version": "1.0.0",
"action": {
    "default_popup": "src/popup/index.html"
},
"side_panel": {
    "default_path": "src/side-panel/index.html"
    },
    "background": {
  "service_worker": "background.js",
  "type": "module"
},
  "icons": {
    "128": "Pick_n_Pay_logo_350_2.png"
  },
  "commands": {
    "reload": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      },
      "description": "Reload the extension" 
    }
  },
"permissions": ["storage", "tabs", "sidePanel", "scripting", "commands"]

}